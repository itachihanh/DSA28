#include<bits/stdc++.h>
using namespace std;
#define ll long long
const int mod = 1e9 + 7;

int main() {
    int n, s;
    cin >> n >> s;
    int a[n + 1];
    for(int i = 1; i <= n; i++) {
        cin >> a[i];
    }
    vector<bool> v(s + 1, false);
    v[0] = true;
    for(int i = 1; i <= n; i++) {
        for(int j = s; j >= a[i]; j--) {
            if(v[j - a[i]]) v[j] = true;
        }
    }
    cout << v[s];
}
