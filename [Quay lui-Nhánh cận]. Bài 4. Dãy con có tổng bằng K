#include<bits/stdc++.h>
using namespace std;

int X[100], a[100], n, k;
bool check = false;

void Try(int i, int idx, int sum) {
    for(int j = idx; j <= n; j++) {
        if(sum + X[j] <= k) {
            sum += X[j];
            a[i] = X[j];
            if(sum == k) {
                check = true;
                cout << "[";
                for(int pos = 1; pos < i; pos++) {
                    cout << a[pos] << " ";
                }
                cout << a[i] << "]" << endl;
            }
            else Try(i + 1, j + 1, sum);
            sum -= X[j];
        }
    }
}

int main() {
    cin >> n >> k;
    for(int i = 1; i <= n; i++) {
        cin >> X[i];
    }
    sort(X + 1, X + n + 1);
    Try(1, 1, 0);
    if(!check) cout << -1;
}
