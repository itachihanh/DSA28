#include <bits/stdc++.h>
using namespace std;

string s;
int n;
bool final = false;

void ktao() {
    s = string(n, '(');
}

void sinh() {
    int i = n - 1;
    while(i >= 0 && s[i] == ')') {
        s[i] = '(';
        --i;
    }
    if(i < 0) {
        final = true;
    }
    else {
        s[i] = ')';
    }
}

bool check(string s) {
    stack <char> st;
    for(int i = 0; i < s.length(); i++) {
        if(s[i] == '(') {
            st.push(s[i]);
        }
        else {
            if(st.empty()) return false;
            st.pop();
        }
    }
    return st.empty();
}

int main() {
    cin >> n;
    ktao();
    bool check2 = false;
    while(!final) {
        if(check(s)) {
            cout << s << endl;
            check2 = true;
        }
        sinh();
    }
    if(!check2) {
        cout << "NOT FOUND";
    }
}
