#include<bits/stdc++.h>
using namespace std;
#define ll long long

const int maxn = 1000005;
ll a[maxn], BIT[maxn], n;

void update(int pos, ll val) {
    for(; pos <= n; pos += pos & (-pos)) {
        BIT[pos] += val;
    }
}

ll query(int pos) {
    ll sum = 0;
    for(; pos > 0; pos -= pos & (-pos)) {
        sum += BIT[pos];
    }
    return sum;
}

int main() {
    cin >> n;
    int q; cin >> q;
    for(int i = 1; i <= n; i++) {
        cin >> a[i];
        update(i, a[i]);
    }
    while(q--) {
        int x, y; cin >> x >> y;
        cout << query(y) - query(x - 1) << endl;
    }
}
